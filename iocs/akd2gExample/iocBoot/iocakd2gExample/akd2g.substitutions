# MRES -> EGU
# 1.0  -> microdegrees
# 1e-3 -> millidegrees
# 1e-6 -> degrees
file "$(MOTOR)/db/asyn_motor.db"
{
pattern
{N,   M,       DTYP,         PORT,    ADDR,  DESC,          EGU,  DIR,  VELO,  VBAS,  ACCL,  BDST,  BVEL,  BACC,  MRES,  PREC, DHLM,  DLLM, RTRY, INIT}
{1,  "m$(N)",  "asynMotor",  AKD2G_1,    0,  "motor $(N)",  deg,  Pos,     1,   .1,     .2,     0,     1,    .2,  1.e-06,   6,  100,  -100,    0,   ""}
}
